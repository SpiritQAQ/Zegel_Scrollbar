{
  "manifest_version": 2,

  "name": "zegel_scrollBar",
  "description": "Personal ScrollBar For Zegel",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [  
    {                  
      "matches":[
        "<all_urls>"
      ], 
      "css": [
      "scrollBar.css"
      ]
    }
  ]
}